{
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "emitDeclarationOnly": false,
    "module": "esnext",
    "target": "es2020",
    "jsx": "react-jsx",
    "moduleResolution": "node",
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
    "skipLibCheck": true,
  "strict": true,
  "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": [
    "src/ui/**/*"
  ]
}
