{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":semanticCommits"],
  "rangeStrategy": "bump",
  "timezone": "America/Denver",
  "labels": ["deps"],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "groupName": "npm minor & patch",
      "matchUpdateTypes": ["minor", "patch"]
    }
  ]
}
